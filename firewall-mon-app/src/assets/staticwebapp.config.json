{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/.auth/login/github",
      "statusCode": 403
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/aad?post_login_redirect_uri=.referrer",
      "statusCode": 302
    },
    "404": {
      "redirect": "/",
      "statusCode": 302
    }
  }
}